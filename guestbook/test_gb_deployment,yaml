apiVersion: apps/v1
kind: Deployment
metadata:
  name: test-gb-ui
spec:
  replicas: 1
  revisionHistoryLimit: 3
  selector:
    matchLabels:
      app: test-gb--ui
  template:
    metadata:
      labels:
        app: test-gb-ui
    spec:
      containers:
      - image: gcr.io/heptio-images/ks-guestbook-demo:0.2
        name: test-gb-ui
        ports:
        - containerPort: 80
        resources:
            requests:
                cpu: 500m
                memory: 0.5 Gi
            limits:
                cpu: 1
                memory: 1 Gi
